{
  "private": true,
  "name": "@one-click-magda/web-client",
  "description": "The One Click Magda web front end.",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "pancake && yarn run start",
    "prepare": "pancake",
    "start": "craco start --react-scripts ../node_modules/react-scripts",
    "build": "pancake && craco build --react-scripts ../node_modules/react-scripts",
    "test": "craco test --react-scripts ../node_modules/react-scripts",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@gov.au/accordion": "^7.0.7",
    "@gov.au/animate": "^1.0.0",
    "@gov.au/body": "^2.1.1",
    "@gov.au/breadcrumbs": "^3.0.4",
    "@gov.au/buttons": "^3.0.6",
    "@gov.au/control-input": "^3.0.1",
    "@gov.au/core": "^3.2.0",
    "@gov.au/cta-link": "^2.1.9",
    "@gov.au/footer": "^3.0.3",
    "@gov.au/grid-12": "^2.0.5",
    "@gov.au/header": "^4.1.11",
    "@gov.au/link-list": "^3.0.5",
    "@gov.au/page-alerts": "^2.1.2",
    "@gov.au/pancake": "^1.2.2",
    "@gov.au/pancake-json": "^1.0.5",
    "@gov.au/pancake-react": "^1.1.2",
    "@gov.au/pancake-sass": "^1.3.2",
    "@gov.au/select": "^2.0.10",
    "@gov.au/skip-link": "^2.0.15",
    "@gov.au/tags": "^3.1.6",
    "@gov.au/text-inputs": "^2.0.11",
    "chrono-node": "^1.3.11",
    "core-js": "^2.5.7",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.1",
    "react-async-hook": "^3.4.0",
    "react-select": "^3.0.4",
    "source-map-explorer": "^1.7.0",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@craco/craco": "^5.0.2",
    "@types/react": "^16.8.17",
    "@types/react-router": "^5.0.3",
    "@types/react-select": "^3.0.0",
    "babel-core": "^6.26.0",
    "browser-detect": "^0.2.22",
    "eslint": "^5.14.1",
    "lodash": "^4.17.10",
    "npm-run-all": "^4.0.1",
    "prop-types": "^15.6.1",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.3",
    "react-responsive": "^4.1.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.8",
    "typescript": "^3.5.3",
    "uuid": "^3.0.1"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11",
    "chrome 41"
  ],
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "json": {
      "enable": true,
      "location": "src/pancake/",
      "name": "pancake",
      "content": {
        "name": true,
        "version": true,
        "dependencies": true,
        "path": true,
        "settings": true
      }
    },
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "src/pancake/css/",
      "name": false
    },
    "sass": {
      "modules": true,
      "location": "src/pancake/sass/",
      "name": "pancake.scss"
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "src/pancake/js/",
      "name": "pancake.min.js"
    },
    "react": {
      "location": "src/pancake/react/"
    }
  },
  "config": {
    "magda": {
      "language": "javascript",
      "categories": {
        "ui": true
      }
    }
  }
}
